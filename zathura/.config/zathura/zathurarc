set recolor true

# Carbon Fox Colors for Zathura
# Default colors
set default-bg                  "#161616" # background
set default-fg                  "#f2f4f8" # foreground
# Render loading colors
set render-loading              true
set render-loading-fg           "#f2f4f8" # foreground
set render-loading-bg           "#161616" # background
# Completion colors
set completion-bg               "#161616" # background
set completion-fg               "#f2f4f8" # foreground
set completion-group-bg         "#161616" # background
set completion-group-fg         "#78A9FF" # blue
set completion-highlight-bg     "#2a2a2a" # selection background
set completion-highlight-fg     "#f2f4f8" # foreground
